{
  "name": "potion-dapp",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "serve:ganache": "vue-cli-service serve --mode ganache",
    "serve:kovan": "vue-cli-service serve --mode kovan",
    "serve:kovan.demo": "vue-cli-service serve --mode kovan.demo",
    "serve:kovan.independent": "vue-cli-service serve --mode kovan.independent",
    "build": "vue-cli-service build",
    "build:kovan": "vue-cli-service build --mode kovan",
    "build:kovan.demo": "vue-cli-service build --mode kovan.demo",
    "build:kovan.independent": "vue-cli-service build --mode kovan.independent",
    "test:unit": "vue-cli-service test:unit",
    "lint": "vue-cli-service lint",
    "prepare:env": "cp .env.ganache.dist .env.ganache && cp .env.kovan.dist .env.kovan && cp .env.kovan.independent.dist .env.kovan.independent && cp .env.kovan.demo.dist .env.kovan.demo",
    "prepare:hooks": "npx @arkweid/lefthook install && npx @arkweid/lefthook run pre-commit",
    "tailwind-config-viewer": "tailwind-config-viewer -o",
    "postinstall": "yarn prepare:hooks"
  },
  "dependencies": {
